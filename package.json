{
  "name": "nativescript-drop-down",
  "version": "5.0.6",
  "description": "A NativeScript DropDown widget.",
  "main": "drop-down",
  "typings": "drop-down.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "4.0.0",
      "android": "4.0.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Interface"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/NativeScript-Drop-Down"
  },
  "scripts": {
    "tslint": "tslint --project tsconfig.json",
    "tsc": "tsc --project tsconfig.json",
    "ngc": "ngc --project tsconfig.aot.json",
    "demo-ios": "grunt build && cd demo && tns run ios",
    "demo-android": "grunt build && cd demo && tns run android",
    "demo-ng-ios": "grunt build && cd demo-ng && tns run ios",
    "demo-ng-android": "grunt build && cd demo-ng && tns run android",
    "postinstall": "npm run ngcc",
    "ngcc": "ngcc --properties es2015 module main --first-only"
  },
  "keywords": [
    "NativeScript",
    "DropDown",
    "Spinner",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "contributors": [
    "Dick Smith (https://github.com/DickSmith)",
    "AWhiteSheep (https://github.com/AWhiteSheep)"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "tns-core-modules": ">= 5"
  },
  "dependencies": {
    "@angular/common": "^10.1.2",
    "@angular/compiler": "^10.1.2",
    "@angular/compiler-cli": "^10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/forms": "^10.1.2",
    "@angular/platform-browser": "^10.1.2",
    "@angular/platform-browser-dynamic": "^10.1.2",
    "@angular/router": "^10.1.2",
    "@nativescript/angular": "10.1.5",
    "@nativescript/core": "7.0.5",
    "@nativescript/types": "7.0.0",
    "@nativescript/ios": "7.0.0",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "rxjs": "~6.5.5",
    "tslint": "^6.1.0",
    "typescript": "^4.0.3",
    "zone.js": "~0.11.1"
  }
}
